<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<settings>
<setting name="cacheEnabled" value="true"/>
<setting name="lazyLoadingEnabled" value="false"/>
<setting name="multipleResultSetsEnabled" value="true"/>
<setting name="useColumnLabel" value="true"/>
<setting name="useGeneratedKeys" value="false"/>
<setting name="defaultExecutorType" value="SIMPLE"/>
<setting name="defaultStatementTimeout" value="25000"/>
<!-- 오라클 컬럼이 first_name 를 VO firstName 처럼 카멜케이스로 변환 -->
<setting name="mapUnderscoreToCamelCase" value="true"/>
</settings>
<typeAliases>
<!--
 	<typeAlias alias="board" type="com.company.app.board.BoardVO"/>
	<typeAlias alias="user" type="com.company.app.user.UserVO"/>
 -->
 	<typeAlias alias="user" type="com.dsup.dbmanagement.UserVO"/> <!-- 무겸형님꺼 -->
 
 	<typeAlias alias="chat" type="com.dsup.chat.ChatVO"/>
 	<typeAlias alias="search" type="com.dsup.chat.SearchVO"/>
 	<typeAlias alias="rp" type="com.dsup.chat.RpVO"/>
 	<typeAlias alias="re" type="com.dsup.chat.ReVO"/>
 	
 	<typeAlias alias="tablespace" type="com.dsup.dbmanagement.TablespaceVO"/>
 	<typeAlias alias="backup" type="com.dsup.dbmanagement.BackupVO"/>
 	<typeAlias alias="datafile" type="com.dsup.dbmanagement.DatafileVO"/>
 	<typeAlias alias="schema" type="com.dsup.distributing.SchemaVO"/>
 	<typeAlias alias="member" type="com.dsup.member.MemberVO"/>
 	<typeAlias alias="pay" type="com.dsup.pay.PayVO"/>
 	<typeAlias alias="userTbspcTb" type="com.dsup.dbmanagement.UserTbspcTbVO"/>
 </typeAliases>
</configuration>
